<div class="panel panel-primary std-panel">
    <div class="panel-heading">
        <h4>
            {% block panel_title %}
            {% endblock %}
        </h4>
    </div>
    <div class="panel-body">
        {% block panel_body %}
        {% endblock %}
    </div>
    <div class="panel-footer">
        <a href="{% spaceless %}{% block back_button_url %}{{ path('index_page') }}{% endblock %}{% endspaceless %}" class=" btn btn-info btn-form">{% spaceless %}{% block back_button %}Повернутися{% endblock %}{% endspaceless %}</a>
        <input type="submit" value="{% spaceless %}{% block submit_button %}Готово{% endblock %}{% endspaceless %}" class="btn btn-success btn-form pull-right" />
    </div>
</div>